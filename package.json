{
  "name": "@damilary/technexus-blog",
  "version": "0.1.0",
  "private": false,
  "description": "Frontend application for the TechNexus Blog, built with Next.js and React.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "npx husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:seed": "tsx backend/prisma/seed.ts",
    "dev:backend": "tsx watch backend/src/index.ts"
  },
  "vitest": {
    "environment": "jsdom",
    "include": [
      "**/*.vitest.test.{ts,tsx,js,jsx}"
    ]
  },
  "dependencies": {
    "@apollo/client": "latest",
