    "helmet": "latest",
    "jsonwebtoken": "latest",
    "lucide-react": "latest",
    "monaco-editor": "latest",
    "mongoose": "latest",
    "next": "latest",
    "next-themes": "latest",
    "prismjs": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "latest",
    "react-hot-toast": "latest",
    "react-intersection-observer": "latest",
    "react-markdown": "latest",
    "react-simplemde-editor": "latest",
    "sharp": "latest",
    "tailwind-merge": "latest",
